<title>Making Ajax Call</title>
<script type="text/javascript" src="Scripts/jquery.js"></script>

<script type="text/javascript">

$(function(){
	$("a").on('click', function(event){
		event.preventDefault();
		var content = $(this).data('file');
		$("#target").html(content);
	})
});


</script>

<style type="text/css">

</style>

